    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-stage-2": "^7.0.0-rc.2",
    "@babel/runtime": "^7.0.0-rc.2",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0-beta.6",
    "browser-sync": "^2.24.5",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
